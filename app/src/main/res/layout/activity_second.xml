            android:textColor="@color/black"
            android:textSize="19sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/iv_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_margin="7dp"
            android:src="@mipmap/icon_xiayibu"
            app:layout_constraintBottom_toBottomOf="@id/tv_multi_1"
            app:layout_constraintLeft_toRightOf="@id/tv_multi_1"
            app:layout_constraintTop_toTopOf="@id/tv_multi_1" />

        <TextView
            android:id="@+id/tv_multi_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textColor="@color/black"
            android:textSize="19sp"
            app:layout_constraintBottom_toBottomOf="@id/tv_multi_1"
            app:layout_constraintLeft_toRightOf="@+id/iv_1"
            app:layout_constraintTop_toTopOf="@id/tv_multi_1" />

        <ImageView
            android:id="@+id/iv_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_margin="7dp"
            android:src="@mipmap/icon_xiayibu"
            app:layout_constraintBottom_toBottomOf="@id/tv_multi_2"
